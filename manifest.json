      }, {
        "id": "get_calendarlist",
        "tql": {
          "parameters": [{
            "name": "vehicle",
            "type": "STRING"
          }],
          "tql": "SELECT * FROM google_calendar.get_calendarlist where summary in (@vehicle)"
        }
      }, {
        "id": "create_calendar_event",
        "tql": {
          "parameters": [{
            "name": "vehicle",
            "type": "STRING"
          }, {
            "name": "requester",
            "type": "STRING"
          }],
          "tql": "SELECT * FROM google_calendar.create_calendar_event\n  WHERE calendarId=(SELECT id from this.get_calendarlist where vehicle=@vehicle)\n  AND $body.summary='test'\n  AND $body.start=(SELECT {\n    'dateTime' : '2020-01-21T08:00:00-08:00'\n  })\n  AND $body.end=(SELECT {\n    'dateTime' : '2020-01-21T10:00:00-08:00'\n  })\n  AND $body.attendees=(SELECT [{'email': @requester}])\n\n/*\n * For sample code and reference material, visit\n * https://www.transposit.com/docs/get-started/sql-quickstart\n */"
        }
      }, {
        "id": "list_cars",
        "function": {
          "js": "get_all_cars.js",
          "lang": "js"
        }
      }, {
        "id": "list_car_calendarlist",
        "function": {
          "js": "list_car_calendarlist.js",
          "lang": "js"
        }
      }, {
