        "id": "get_message",
        "tql": {
          "tql": "SELECT * FROM google_mail.get_message\n  WHERE userId='me'\n  AND id='16fc6048e653d2ef'\n  AND format='full'\n\n/*\n * For sample code and reference material, visit\n * https://www.transposit.com/docs/get-started/sql-quickstart\n */"
        }
      }, {
        "id": "list_history_of_mailbox",
        "tql": {
          "tql": "SELECT * FROM google_mail.list_history_of_mailbox\n  WHERE userId='me'\n  AND startHistoryId='3681'\n\n/*\n * For sample code and reference material, visit\n * https://www.transposit.com/docs/get-started/sql-quickstart\n */"
        }
      }, {
